import lombok.Data;
import lombok.EqualsAndHashCode;

@SuppressWarnings("serial")
@Data
@EqualsAndHashCode(callSuper=false)
public class SpdPropertyChangeRecordParams extends QueryParams{
	private ListParam<String> id;
	private ListParam<String> dailyCleanId;
	private ListParam<String> propertyChangeRuleId;
	private ListParam<String> bizTypeCode;
	private ListParam<String> bizId;
	private ListParam<String> taskId;
	private ListParam<String> triggerBizStepCode;
	
	private ListParam<String> creditOrgId;
	private ListParam<String> debitOrgId;
	private ListParam<String> debitDeptId;
	private ListParam<String> debitDepotId;
	private ListParam<String> creditDepotId;
	private ListParam<String> creditDeptId;
	private DateRangeParam targetDate;
	private DateRangeParam createTime;
	
	private ListParam<String> creditOrgName;
	private ListParam<String> creditDeptName;
	private ListParam<String> debitOrgName;
	private ListParam<String> debitDeptName;
	private ListParam<String> debitDepotName;
	private ListParam<String> creditDepotName;
}
