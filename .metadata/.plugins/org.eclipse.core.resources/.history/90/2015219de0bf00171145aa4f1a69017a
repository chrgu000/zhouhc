--2017-7-3
--1)更新业务规则的enabled的值
update spd_adjust_stock_rule sasr set sasr.enabled = '1' where sasr.rule_prior = '99';
update spd_adjust_stock_rule sasr set sasr.enabled = '0' where sasr.rule_prior = '1';

update spd_move_area_rule smar set smar.enabled = '1' where smar.rule_prior = '99';
update spd_move_area_rule smar set smar.enabled = '0' where smar.rule_prior = '1';

update spd_receive_rule srr set srr.enabled = '1' where srr.rule_prior = '99';
update spd_receive_rule srr set srr.enabled = '0' where srr.rule_prior = '1';

--2)更新默认用的用户名及邮箱地址
update pcs_user ur set ur.email='webmaster@syshospital.com';
update pcs_user ur set  (ur.user_name)= (select tb.user_name from (select dept.dept_name||'代理用户' user_name,dept.dept_id from pcs_dept dept) tb where ur.dept_id=tb.dept_id )where ur.user_id like ('user_id_%') and ur.user_id not in ('user_id_001','user_id_002')

--3)手动为库房添加收货规则查看、库存调节规则查看、移仓规则查看权限

