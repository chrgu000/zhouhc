--移仓规则
ALTER TABLE SPD_MOVE_AREA_RULE RENAME COLUMN "FROM_AREA_ID" TO FROM_AREA_TYPE;
ALTER TABLE SPD_MOVE_AREA_RULE RENAME COLUMN "TO_AREA_ID" TO TO_AREA_TYPE;

--备货规则
ALTER TABLE SPD_PREPARE_INFORM_RULE RENAME COLUMN "TO_AREA_ID" TO TO_AREA_TYPE;
ALTER TABLE SPD_PREPARE_INFORM_RULE RENAME COLUMN "COLLECT_AREA_ID" TO COLLECT_AREA_TYPE;

--收货规则
ALTER TABLE SPD_RECEIVE_RULE RENAME COLUMN "TO_AREA_ID" TO TO_AREA_TYPE;
ALTER TABLE SPD_RECEIVE_RULE RENAME COLUMN "FINAL_AREA_ID" TO FINAL_AREA_TYPE;

--退货规则
ALTER TABLE SPD_RETURN_GOODS_RULE RENAME COLUMN "FROM_AREA_ID" TO FROM_AREA_TYPE;

